{
	"extends": "next/core-web-vitals",
	"rules": {
		"no-console": "error",
		"no-unused-vars": "error",
		"quotes": ["error", "double"],
		"jsx-quotes": ["error", "prefer-double"],
		"prefer-const": "error",
		"indent": ["error", "tab"],
		"comma-dangle": ["error", "always-multiline"],
		"semi": ["error", "always"],
		"import/order": [
			"error",
			{
				"groups": ["builtin", "external", "parent", "sibling", "index"],
				"newlines-between": "always-and-inside-groups"
			}
		]
	}
}
